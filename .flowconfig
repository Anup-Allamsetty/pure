[ignore]

# Ignore test files
.*/__tests__/*
.*/tests/*
.*/test/*

# Ignore fbs and babel stuff
.*/node_modules/fbjs.*
.*/node_modules/babel.*

# Ugh
.*/node_modules/binary-extensions/.*.json
.*/node_modules/builtin-modules/.*.json
.*/node_modules/iconv-lite/.*.json
.*/node_modules/resolve/.*.json
.*/node_modules/spdx-exceptions/.*.json
.*/node_modules/spdx-license-ids/.*.json
.*/node_modules/tr46/.*.json
.*/node_modules/url-regex/.*.json

[include]

[libs]
interfaces/

[options]
munge_underscores=true

module.name_mapper='^\..+/[a-zA-Z0-9$_-]+$' -> '\0.web.js'
module.name_mapper='^\..+/[a-zA-Z0-9$_-]+$' -> '\0.ios.js'
module.name_mapper='^\..+/[a-zA-Z0-9$_-]+$' -> '\0.android.js'

module.name_mapper='^image![a-zA-Z0-9$_-]+$' -> 'GlobalImageStub'
module.name_mapper='^[./a-zA-Z0-9$_-]+\.png$' -> 'RelativeImageStub'

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(>=0\\.\\(1[0-9]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*react_native[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(>=0\\.\\(1[0-9]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*react_native[a-z,_]*\\)?)\\)? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy

esproposal.export_star_as=enable
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
